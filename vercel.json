{
  "version": 2,
  "builds": [
    {
      "src": "web_interface_vercel_simple.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web_interface_vercel_simple.py"
    }
  ],
  "env": {
    "HOST": "0.0.0.0",
    "PORT": "8080",
    "DEBUG": "false"
  }
}
